{
  "data_config": {
    "data_name": "iaac",
    "train_dataset_path": "/MMLRec/data/iaac_train.csv",
    "test_dataset_path": "/MMLRec/data/iaac_test.csv",
    "test_result_path": "/MMLRec/results/iaac_test_result.csv",
    "layer_output_path": "/MMLRec/results/iaac_layer_output_msl/",
    "all_columns": ["item_category_list", "item_property_list",
       "item_brand_id", "item_city_id", "item_price_level", "item_sales_level",
       "item_collected_level", "item_pv_level", "user_gender_id",
       "user_age_level", "user_occupation_id", "user_star_level", "context_id",
       "context_timestamp", "context_page_id", "predict_category_property",
       "shop_id", "shop_review_num_level","shop_star_level", "shop_review_positive_rate", 
       "shop_score_service", "shop_score_delivery", "shop_score_description", "is_trade"],
    "feature_columns": ["item_category_list", "item_property_list",
       "item_brand_id", "item_city_id", "item_price_level", "item_sales_level",
       "item_collected_level", "item_pv_level", 
       "user_age_level", "user_occupation_id", "user_star_level", "context_id",
       "context_timestamp", "context_page_id", "predict_category_property",
       "shop_id", "shop_review_num_level","shop_star_level"],
    "dense_columns": ["shop_review_positive_rate", "shop_score_service", "shop_score_delivery", "shop_score_description"],
    "ignore_columns": [],
    "label_columns": ["is_trade", "is_trade", "is_trade", "is_trade"],
    "sample": "random",
    "num_domains": 4,
    "mask_values": [0, 1, 2, 3],
    "mask_column": "user_gender_id",
    "scene_feature": "user_gender_id"
  },
  "model_config": {
    "task_name": "msl",
    "model_name": "snr_trans",
